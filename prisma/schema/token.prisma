model Token {
  id         String   @id @default(uuid()) @db.Uuid
  sessions   Session? @relation(name: "sessions_to_token", fields: [sessionId], references: [id], onDelete: Cascade)
  sessionId  String?  @db.Uuid
  hash       String
  createdAt  DateTime @default(now()) @map("created_at")

  @@index([sessionId])
  @@map("tokens")
}
